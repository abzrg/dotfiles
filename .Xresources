!! Extensions -----------------------------------------------------------
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:      #00486B

!! matcher
URxvt.url-launcher: /usr/bin/xdg-open
!   on first click do the url-launcher
URxvt.matcher.button: 1
!   different color for links
URxvt.matcher.rend.0: Uline Bold fg5
!   open and list recent links
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

!! keyboard-select:
URxvt.keysym.M-v: perl:keyboard-select:activate

!! tabs
!URxvt.tabbed.tabbar-fg: 2
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg: 8
!URxvt.tabbed.tab-bg: 3

!! resize-font
URxvt.resize-font.step: 1
URxvt.resize-font.smaller:  C-Down
URxvt.resize-font.bigger:   C-Up

!! disable iso ----------------------------------------------------------
URxvt*iso14755        : false
URxvt*iso14755_52     : false

!! some key-binding -----------------------------------------------------
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
! move one line up and down
URxvt.keysym.M-k: command:\033]720;1\007
URxvt.keysym.M-j: command:\033]721;1\007

